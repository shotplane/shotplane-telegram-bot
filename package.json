  "scripts": {
    "dev": "concurrently -k -p \"[{name}]\" -n \"Ts,node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-dev\"",
    "watch-ts": "rimraf dist && tsc -w",
    "clean": "rimraf dist/",
    "copy-swagger": "copyfiles -u 1 src/docs/swagger.yaml dist/",
    "watch-dev": "nodemon --watch \"dist/**/*\" --delay 2000ms -e  js ./dist/server.js",
    "build-ts": "rimraf dist && tsc",
    "start": "NODE_ENV=production node dist/server.js",
    "lint": "eslint \"**/*.{js,ts}\" --quiet --fix",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"sass,Ts,node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "prettier": "prettier --write '**/*.{ts,css,js}'",
    "test": "mocha src/test/nft.ts"
  },
